doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    title Upload
  body
    h1 Uploaded Files
    ul
      each file in files
        li= file
      else 
        h3 No files uploaded
    a(href="/upload") upload file
